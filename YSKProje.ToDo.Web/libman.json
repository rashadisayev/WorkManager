{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.6.0",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [ "jquery.min.js" ]
    },
    {
      "library": "jquery-validate@1.19.2",
      "destination": "wwwroot/lib/jqueryValidate",
      "files": [ "jquery.validate.min.js" ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jqueryUnob",
      "files": [ "jquery.validate.unobtrusive.min.js" ]
    },
    {
      "library": "font-awesome@5.15.3",
      "destination": "wwwroot/lib/fontAwesome",
      "files": [ "css/all.css", "js/all.js", "webfonts/fa-solid-900.woff", "webfonts/fa-solid-900.woff2",
                 "webfonts/fa-regular-400.woff", "webfonts/fa-regular-400.woff2", "webfonts/fa-solid-900.ttf" ]

    }
  ]
}